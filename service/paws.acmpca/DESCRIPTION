Package: paws.acmpca
Title: AWS Certificate Manager Private Certificate Authority
Version: 0.0.2
Authors@R: 
    c(person(given = "David",
             family = "Kretch",
             role = c("aut", "cre"),
             email = "david.kretch@gmail.com"),
