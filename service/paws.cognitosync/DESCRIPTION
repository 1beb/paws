Package: paws.cognitosync
Title: Amazon Cognito Sync
Version: 0.0.0.9000
Authors@R: 
    c(person(given = "David",
             family = "Kretch",
             role = c("aut", "cre"),
             email = "david.kretch@gmail.com"),
      person(given = "Adam",
             family = "Banker",
