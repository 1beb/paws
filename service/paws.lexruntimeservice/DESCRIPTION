Package: paws.lexruntimeservice
Title: Amazon Lex Runtime Service
Version: 0.0.2
Authors@R: 
    c(person(given = "David",
             family = "Kretch",
             role = c("aut", "cre"),
             email = "david.kretch@gmail.com"),
