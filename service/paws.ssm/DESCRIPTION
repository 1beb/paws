Package: paws.ssm
Title: Amazon Simple Systems Manager (SSM)
Version: 0.0.2
Authors@R: 
    c(person(given = "David",
             family = "Kretch",
             role = c("aut", "cre"),
             email = "david.kretch@gmail.com"),
